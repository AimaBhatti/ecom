{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "shipment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shipment.shipment",
      "inversedBy": "order"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "In Process",
        "Shipped",
        "Completed"
      ],
      "required": false,
      "default": "In Process"
    },
    "confirmed": {
      "type": "boolean",
      "default": true
    },
    "pay_type": {
      "type": "enumeration",
      "enum": [
        "COD",
        "Stripe",
        "Easypaisa"
      ]
    },
    "order_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-detail.order-detail",
      "mappedBy": "order_id"
    },
    "grand_total": {
      "type": "decimal"
    },
    "user_id": {
      "type": "string",
      "required": true
    }
  }
}
