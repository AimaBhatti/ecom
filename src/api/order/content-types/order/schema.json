{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "order_Id": {
      "type": "uid",
      "required": true,
      "minLength": 6
    },
    "grand_total": {
      "required": true,
      "type": "decimal"
    },
    "order_detail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order-detail.order-detail",
      "inversedBy": "order_Id"
    },
    "shipment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shipment.shipment",
      "inversedBy": "order"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "In Process",
        "Shipped",
        "Completed"
      ],
      "required": false,
      "default": "In Process"
    },
    "paymentInfo": {
      "type": "json"
    },
    "user_Id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    }
  }
}
